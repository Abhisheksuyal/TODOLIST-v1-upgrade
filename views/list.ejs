<%- include("header") -%>

<div class="box" id="heading">
  <h1> <%= kindOfDay %> </h1>
</div>
<div class="box">
  <%  newListItem.forEach(function(i){ %>
    <form action="/delete" method="post">


  <div class="item">
    <input type="checkbox" name="check" value="<%=i._id %>" onChange="this.form.submit()">
    <p> <%=i.name%> </p>
  </div>

  </form>
  <%});%>
  <form class="item" action="/" method="post">
    <input type="text" name="rece" autocomplete="off" placeholder="New Item">
    <button type="submit" name="btn" value="<%= kindOfDay %>">+</button>
  </form>
</div>

<%- include("footer") -%>
